        "required": [
          "name"
        ]
      },
      "UpdateFlowDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Updated Flow Name"
          },
          "description": {
            "type": "string",
            "example": "Updated description"
          },
          "nodes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/NodeDto"
            }
          },
          "edges": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EdgeDto"
            }
          },
          "viewport": {
            "$ref": "#/components/schemas/ViewportDto"
          },
          "variables": {
            "type": "array",
